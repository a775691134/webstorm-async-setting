<keymap version="1" name="Default for Windows copy" parent="$default">
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="alt f12" />
    <keyboard-shortcut first-keystroke="ctrl back_quote" />
  </action>
  <action id="Find">
    <keyboard-shortcut first-keystroke="ctrl f" />
  </action>
  <action id="FindInPath">
    <keyboard-shortcut first-keystroke="ctrl alt f" />
  </action>
  <action id="FindWordAtCaret">
    <keyboard-shortcut first-keystroke="shift ctrl f" />
  </action>
  <action id="IntroduceField" />
  <action id="Javascript.Linters.EsLint.Fix">
    <keyboard-shortcut first-keystroke="shift ctrl alt z" />
    <keyboard-shortcut first-keystroke="ctrl numpad5" />
  </action>
  <action id="QuickChangeScheme" />
  <action id="ShowFilterPopup" />
  <action id="TsLintFileFixAction">
    <keyboard-shortcut first-keystroke="ctrl numpad1" />
  </action>
</keymap>